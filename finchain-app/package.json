{
  "name": "finchain-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-image": "docker build -t finchain-dapp-image .",
    "remove-image": "docker rmi finchain-dapp-image",
    "run-container": "docker run --name finchain-dapp-container -d -p 4200:80 finchain-dapp-image",
    "remove-container": "docker rm -f finchain-dapp-container",
    "containerize": "npm run build-image && npm run run-container",
    "recontainerize": "npm run remove-container && npm run build-image && npm run run-container"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.2.3",
    "jquery": "^3.7.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/cli": "^17.3.2",
    "@angular/compiler-cli": "^17.3.6",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.19",
    "crypto-js": "^4.2.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.2",
    "typescript": "~5.4.2"
  }
}